  "bin": {
    "always-onward": "bin/always-onward.js"
  },
  "scripts": {
    "clean": "rm -r cdk.out && rm -r dist",
    "test-cdk": "cdk synth",
    "test-route": "npm --prefix routeLambda/ run test",
    "test-processor": "cd processorLambda && pipenv run test && cd ..",
    "test-van": "cd vanComputer && pipenv run test && cd ..",
    "ssh-van": "ssh pi@vancomputer.always-onward.com -p 7056",
    "test": "npm run test-route && npm run test-processor && npm run test-van && npm run test-cdk",
    "serve": "nodemon -L local.js server",
    "van": "cd vanComputer && pipenv run py index.py && cd ..",
    "van-ota": "cd vanComputer && pipenv run py ota.py && cd ..",
    "test-route-deploy": "npm --prefix routeLambda/ run deploy-test",
    "deploy": "cdk deploy --all --require-approval never"
  },
